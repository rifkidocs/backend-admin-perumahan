{
  "kind": "collectionType",
  "collectionName": "proyek_perumahans",
  "info": {
    "singularName": "proyek-perumahan",
    "pluralName": "proyek-perumahans",
    "displayName": "Proyek Perumahan",
    "description": "Project management system for housing development"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "project_id": {
      "type": "string",
      "required": false,
      "unique": true,
      "regex": "^PROJ-[0-9]{6}$"
    },
    "project_name": {
      "type": "string",
      "minLength": 2,
      "maxLength": 100,
      "required": true
    },
    "project_type": {
      "type": "enumeration",
      "required": true,
      "enum": ["perumahan", "pembangunan", "renovasi"]
    },
    "jenis_proyek": {
      "type": "enumeration",
      "enum": ["Subsidi", "Komersial", "Mixed-Use"]
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "estimated_completion": {
      "type": "date",
      "required": true
    },
    "actual_completion": {
      "type": "date"
    },
    "budget": {
      "type": "text",
      "required": true
    },
    "current_expense": {
      "type": "text"
    },
    "status_proyek": {
      "type": "enumeration",
      "required": true,
      "enum": ["perencanaan", "pembangunan", "terjual habis"]
    },
    "total_units": {
      "type": "integer",
      "min": 0
    },
    "completed_units": {
      "type": "integer",
      "min": 0
    },
    "progress_percentage": {
      "type": "decimal",
      "min": 0,
      "max": 100
    },
    "investment_value": {
      "type": "text"
    },
    "contact_info": {
      "type": "json"
    },
    "project_description": {
      "type": "text",
      "maxLength": 1000
    },
    "address": {
      "type": "text",
      "maxLength": 500
    },
    "coordinate_lat": {
      "type": "decimal",
      "min": -90,
      "max": 90
    },
    "coordinate_lng": {
      "type": "decimal",
      "min": -180,
      "max": 180
    },
    "building_license": {
      "type": "string",
      "maxLength": 50
    },
    "environment_permits": {
      "type": "json"
    },
    "zoning_type": {
      "type": "enumeration",
      "enum": ["residensial", "komersial", "campuran"]
    },
    "land_area": {
      "type": "text"
    },
    "notes": {
      "type": "text",
      "maxLength": 1000
    },
    "unit_types": {
      "type": "json",
      "required": false
    },
    "foto_utama": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "galeri_foto": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "site_plan": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "dokumen_legal": {
      "type": "component",
      "component": "komponen.dokumen",
      "repeatable": true
    },
    "developer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::developer.developer",
      "inversedBy": "proyek_perumahans"
    },
    "units": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::unit-rumah.unit-rumah",
      "mappedBy": "proyek_perumahan"
    },
    "manager_proyek": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::karyawan.karyawan",
      "inversedBy": "proyek_perumahans"
    },
    "karyawans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::karyawan.karyawan",
      "mappedBy": "proyek_ditangani"
    },
    "contractors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::vendor.vendor",
      "inversedBy": "contractor_projects"
    },
    "phases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-phase.project-phase",
      "mappedBy": "proyek_perumahan"
    },
    "documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-document.project-document",
      "mappedBy": "proyek_perumahan"
    },
    "workers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-worker.project-worker",
      "mappedBy": "proyek_perumahan"
    },
    "materials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project-material.project-material",
      "mappedBy": "proyek_perumahan"
    },
    "permintaan_materials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::permintaan-material.permintaan-material",
      "mappedBy": "proyek_perumahan"
    },
    "pengeluaran_materials": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pengeluaran-material.pengeluaran-material",
      "mappedBy": "proyek_perumahan"
    },
    "kas_keluars": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kas-keluar.kas-keluar",
      "mappedBy": "proyek_perumahan"
    },
    "target_marketings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::target-marketing.target-marketing",
      "mappedBy": "proyek_perumahan"
    },
    "rabs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::rab.rab",
      "mappedBy": "proyek_perumahan"
    },
    "progres_harians": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::progres-harian.progres-harian",
      "mappedBy": "proyek_perumahan"
    },
    "unit_pricings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::unit-pricing.unit-pricing",
      "mappedBy": "project"
    },
    "promotions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::promo.promo",
      "mappedBy": "project"
    },
    "site_plan_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::site-plan-document.site-plan-document",
      "mappedBy": "project"
    },
    "technical_drawings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technical-drawing.technical-drawing",
      "mappedBy": "project"
    },
    "permit_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::permit-document.permit-document",
      "mappedBy": "project"
    },
    "boq_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::boq-document.boq-document",
      "mappedBy": "project"
    },
    "handover_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::handover-document.handover-document",
      "mappedBy": "project"
    },
    "work_items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::work-item.work-item",
      "mappedBy": "proyek"
    },
    "labor_estimations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::labor-estimation.labor-estimation",
      "mappedBy": "proyek"
    },
    "brochures": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brochure.brochure",
      "mappedBy": "project"
    },
    "marketing_videos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::marketing-video.marketing-video",
      "mappedBy": "project"
    },
    "pekerjas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::pekerja.pekerja",
      "mappedBy": "proyek"
    },
    "subkontraktors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::subkontraktor.subkontraktor",
      "mappedBy": "proyek"
    },
    "jadwal_kerjas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::jadwal-kerja.jadwal-kerja",
      "mappedBy": "proyek"
    },
    "penugasans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::penugasan.penugasan",
      "mappedBy": "proyek"
    }
  }
}
