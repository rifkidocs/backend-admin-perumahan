    },
    "metode_pembayaran": {
      "type": "enumeration",
      "enum": [
        "Transfer",
        "Tunai",
        "Cek"
      ]
    },
    "keterangan": {
      "type": "string"
    },
    "bukti_pembayaran": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
