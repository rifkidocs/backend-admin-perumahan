{
  "kind": "collectionType",
  "collectionName": "pengeluaran_materials",
  "info": {
    "singularName": "pengeluaran-material",
    "pluralName": "pengeluaran-materials",
    "displayName": "Pengeluaran & Distribusi Material",
    "description": "Sistem Pengeluaran dan Distribusi Material dari gudang ke proyek dengan tracking lengkap"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "mrNumber": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "distributionNumber": {
      "type": "string",
      "required": false,
      "unique": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "time",
      "required": true
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::proyek-perumahan.proyek-perumahan",
      "required": true
    },
    "list_materials": {
      "type": "component",
      "repeatable": true,
      "component": "penerimaan.material-item",
      "required": true
    },
    "requester": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "approver": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::karyawan.karyawan"
    },
    "warehouseSupervisor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::karyawan.karyawan"
    },
    "approvalStatus": {
      "type": "enumeration",
      "enum": ["pending", "approved", "rejected"],
      "default": "pending"
    },
    "status_issuance": {
      "type": "enumeration",
      "enum": [
        "pending",
        "in-transit",
        "delivered",
        "Pending",
        "Sedang Diproses",
        "Selesai"
      ],
      "default": "pending"
    },
    "notes": {
      "type": "text"
    },
    "documents": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files"]
    },
    "driver": {
      "type": "string",
      "maxLength": 255
    },
    "escort": {
      "type": "string",
      "maxLength": 255
    },
    "supervisor": {
      "type": "string",
      "maxLength": 255
    },
    "vehicle": {
      "type": "string",
      "maxLength": 100
    },
    "priorityLevel": {
      "type": "enumeration",
      "enum": ["low", "normal", "high", "urgent"],
      "default": "normal"
    },
    "estimatedArrival": {
      "type": "datetime"
    },
    "actualArrival": {
      "type": "datetime"
    },
    "departureTime": {
      "type": "datetime"
    },
    "deliveryCost": {
      "type": "decimal",
      "min": 0
    },
    "trackingNotes": {
      "type": "text"
    },
    "receiverName": {
      "type": "string",
      "maxLength": 255
    },
    "receiverPosition": {
      "type": "string",
      "maxLength": 255
    },
    "deliveryNotes": {
      "type": "text"
    },
    "vehicleNumber": {
      "type": "string",
      "maxLength": 20
    },
    "vehicleCapacity": {
      "type": "string",
      "maxLength": 50
    },
    "transportType": {
      "type": "string",
      "maxLength": 50
    },
    "needCrane": {
      "type": "boolean",
      "default": false
    },
    "needHelper": {
      "type": "boolean",
      "default": false
    },
    "specialInstructions": {
      "type": "text"
    },
    "unit_rumah": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::unit-rumah.unit-rumah",
      "inversedBy": "pengeluaran_materials"
    },
    "penyetuju": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::karyawan.karyawan",
      "inversedBy": "pengeluaran_materials"
    },
    "proyek_perumahan": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::proyek-perumahan.proyek-perumahan",
      "inversedBy": "pengeluaran_materials"
    },
    "gudang": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::gudang.gudang",
      "inversedBy": "pengeluaran_materials"
    }
  }
}
