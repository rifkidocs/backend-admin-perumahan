{
  "kind": "collectionType",
  "collectionName": "pos_keuangans",
  "info": {
    "singularName": "pos-keuangan",
    "pluralName": "pos-keuangans",
    "displayName": "Pos Keuangan",
    "description": "Manajemen pos keuangan (Kas, Bank, Cek/Giro)"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nama_pos": {
      "type": "string",
      "maxLength": 100,
      "required": true
    },
    "nomor_rekening": {
      "type": "string",
      "maxLength": 50,
      "required": false,
      "unique": true
    },
    "nama_pemilik": {
      "type": "string",
      "maxLength": 200,
      "required": false
    },
    "saldo": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "jenis_rekening": {
      "type": "enumeration",
      "default": "tabungan",
      "enum": [
        "tunai",
        "tabungan",
        "giro",
        "cek",
        "deposito",
        "elektronik"
      ]
    },
    "saldo_minimum": {
      "type": "decimal"
    },
    "status_aktif": {
      "type": "boolean",
      "default": true
    },
    "deskripsi": {
      "type": "text",
      "maxLength": 500
    },
    "kas_keluars": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kas-keluar.kas-keluar",
      "mappedBy": "pos_keuangan"
    },
    "kas_masuks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kas-masuk.kas-masuk",
      "mappedBy": "pos_keuangan"
    }
  }
}
