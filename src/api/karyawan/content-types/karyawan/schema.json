  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "nik_karyawan": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "nama_lengkap": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 100
    },
    "jenis_kelamin": {
      "type": "enumeration",
      "enum": ["Laki-laki", "Perempuan"],
      "required": true
    },
    "tempat_lahir": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 50
    },
    "tanggal_lahir": {
      "type": "date",
      "required": true
    },
    "alamat": {
      "type": "text",
      "required": true,
      "minLength": 10,
      "maxLength": 500
    },
    "kontak": {
      "type": "component",
      "component": "komponen.kontak",
      "repeatable": false
    },
    "status_pernikahan": {
      "type": "enumeration",
      "enum": ["Belum Menikah", "Menikah", "Cerai"],
      "required": true
    },
    "foto_karyawan": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "npwp": {
      "type": "string",
      "regex": "^[0-9]{15}$"
    },
    "kontak_darurat": {
      "type": "text",
      "maxLength": 200
    },
    "status_kepegawaian": {
      "type": "enumeration",
      "enum": ["Tetap", "Kontrak", "Freelance"],
      "required": true
    },
    "tanggal_masuk": {
      "type": "date",
      "required": true
