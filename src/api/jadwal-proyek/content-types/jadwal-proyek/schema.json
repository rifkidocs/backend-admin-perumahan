{
  "kind": "collectionType",
  "collectionName": "jadwal_proyeks",
  "info": {
    "singularName": "jadwal-proyek",
    "pluralName": "jadwal-proyeks",
    "displayName": "Jadwal Proyek",
    "description": "Project timeline and task management"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "project_name": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 100
    },
    "phase_name": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 50
    },
    "task_name": {
      "type": "string",
      "required": true,
      "minLength": 2,
      "maxLength": 200
    },
    "start_date": {
      "type": "date",
      "required": true
    },
    "end_date": {
      "type": "date",
      "required": true
    },
    "duration_days": {
      "type": "integer",
      "required": true,
      "min": 1
    },
    "task_status": {
      "type": "enumeration",
      "enum": ["planned", "in-progress", "completed", "delayed"],
      "required": true,
      "default": "planned"
    },
    "progress_percentage": {
      "type": "decimal",
      "min": 0,
      "max": 100,
      "default": 0
    },
    "priority": {
      "type": "enumeration",
      "enum": ["low", "medium", "high", "urgent"],
      "default": "medium"
    },
    "assigned_to": {
      "type": "string",
      "maxLength": 100
    },
    "notes": {
      "type": "text",
      "maxLength": 500
    },
    "dependencies": {
      "type": "json"
    },
    "actual_start_date": {
      "type": "date"
    },
    "actual_end_date": {
      "type": "date"
    },
    "deviation_days": {
      "type": "integer"
    },
    "proyek_perumahan": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::proyek-perumahan.proyek-perumahan"
    },
    "unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::unit-rumah.unit-rumah"
    },
    "contractor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendor.vendor"
    },
    "phase_relation": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project-phase.project-phase",
      "inversedBy": "tasks"
    }
  }
}
