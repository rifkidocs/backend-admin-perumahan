{
  "kind": "collectionType",
  "collectionName": "commission_structures",
  "info": {
    "singularName": "commission-structure",
    "pluralName": "commission-structures",
    "displayName": "Commission Structure",
    "description": "Define commission structures for different unit types"
  },
