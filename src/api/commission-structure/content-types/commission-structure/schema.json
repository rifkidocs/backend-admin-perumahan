    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "unit_type": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "commission_rate": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 100
    },
    "commission_amount": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "bonus_threshold": {
      "type": "integer",
      "min": 1
    },
    "bonus_rate": {
      "type": "decimal",
      "min": 0
    },
    "cash_incentive": {
      "type": "decimal",
      "min": 0
    },
    "is_active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "effective_date": {
      "type": "date",
      "required": true
    },
    "expiry_date": {
      "type": "date"
    },
    "notes": {
      "type": "text",
      "maxLength": 500
    }
