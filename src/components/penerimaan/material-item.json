{
  "collectionName": "components_penerimaan_material_items",
  "info": {
    "displayName": "Material Item",
    "description": "Item material dalam penerimaan"
  },
  "options": {},
  "attributes": {
    "material": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::material.material"
    },
    "nama_material_baru": {
      "type": "string"
    },
    "quantity": {
      "type": "decimal",
      "required": true,
      "min": 0.01
    },
    "unit": {
      "type": "string",
      "required": true
    },
    "condition": {
      "type": "enumeration",
      "required": true,
      "default": "Baik",
      "enum": [
        "Baik",
        "Rusak",
        "Kurang"
      ]
    },
    "harga_satuan": {
      "type": "biginteger",
      "required": false
    },
    "total_harga": {
      "type": "biginteger",
      "required": false
    }
  }
}
