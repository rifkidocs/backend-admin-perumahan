{
  "collectionName": "components_pengeluaran_item_pengeluarans",
  "info": {
    "displayName": "Item Pengeluaran",
    "icon": "box",
    "description": "Item pengeluaran material (stok atau beli langsung)"
  },
  "options": {},
  "attributes": {
    "sumber": {
      "type": "enumeration",
      "required": true,
      "default": "stok",
      "enum": [
        "stok",
        "langsung_beli"
      ]
    },
    "material_gudang": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::material-gudang.material-gudang"
    },
    "material": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::material.material"
    },
    "quantity": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "unit": {
      "type": "string",
      "required": true
    },
    "condition": {
      "type": "enumeration",
      "required": true,
      "default": "Baik",
      "enum": [
        "Baik",
        "Rusak",
        "Kurang"
      ]
    },
    "harga_satuan": {
      "type": "biginteger",
      "required": false
    },
    "total_harga": {
      "type": "biginteger",
      "required": false
    },
    "keterangan": {
      "type": "text"
    },
    "nota": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    }
  }
}
