{
  "collectionName": "components_payment_invoice_items",
  "info": {
    "displayName": "Invoice Item",
    "description": "Individual line items within a payment invoice"
  },
  "options": {},
  "attributes": {
    "material": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::material.material"
    },
    "description": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "quantity": {
      "type": "decimal",
      "required": true,
      "min": 0.01,
      "max": 1000000
    },
    "unit": {
      "type": "string",
      "required": true,
      "maxLength": 20
    },
    "unitPrice": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 1000000000
    },
    "totalPrice": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "max": 1000000000
    },
    "notes": {
      "type": "string",
      "maxLength": 500
    },
    "goodsReceiptItem": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::penerimaan-material.penerimaan-material"
    }
  }
}